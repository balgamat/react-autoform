{
  "name": "@balgamat/react-autoform",
  "version": "9.0.2",
  "description": "A React component for simple creation of forms to edit any object, that can e.g. serve as the C+U in your CRUD.",
  "author": "Matej Balga",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balgamat/react-autoform.git"
  },
  "bugs": {
    "url": "https://github.com/balgamat/react-autoform.gitissues"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "yarn build",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "homepage": "https://balgamat.github.io/react-autoform/",
  "license": "GPL-3.0",
  "keywords": [
