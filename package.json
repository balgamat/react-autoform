    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/ramda": "^0.26.33",
    "@types/react": "^16.9.9",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.11",
    "jest": "^26.5.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "ts-jest": "^26.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "ramda": "^0.26.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.8"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts"
  }
