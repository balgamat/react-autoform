{
  "name": "react-autoform",
  "version": "1.0.1",
  "description": "A React component for simple creation of forms to edit any object, that can e.g. serve as the C+U in your CRUD.",
  "author": "Matej Balga",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balgamat/react-autoform.git"
  },
  "bugs": {
    "url": "https://github.com/balgamat/react-autoform.gitissues"
  },
  "scripts": {
    "build": "npm version patch && rm -rf ./build && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "yarn build",
    "lint": "tslint -p tsconfig.json"
  },
  "homepage": "https://github.com/balgamat/react-autoform.git#readme",
  "license": "GPL-3.0",
  "keywords": [
    "react-component",
    "react",
    "forms",
    "autoform",
    "automatic form creation",
    "ui"
  ],
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/ramda": "^0.26.33",
    "@types/react": "^16.9.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "react": "^16.8.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}
